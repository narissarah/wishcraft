{
  "buildCommand": "echo 'Build disabled - using API functions only'",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/app.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/registry-simple.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/auth/shopify.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/auth/callback.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/test-db.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/db-status.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/quick-db-check.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/registry-db.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/webhooks/customers-data-request.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/webhooks/customers-redact.js": {
      "runtime": "@vercel/node@3.0.12"
    },
    "api/webhooks/shop-redact.js": {
      "runtime": "@vercel/node@3.0.12"
    }
  },
  "rewrites": [
    {
      "source": "/auth/shopify",
      "destination": "/api/auth/shopify"
    },
    {
      "source": "/auth/callback",
      "destination": "/api/auth/callback"
    },
    {
      "source": "/auth/shopify/callback",
      "destination": "/api/auth/callback"
    },
    {
      "source": "/app",
      "destination": "/api/app"
    },
    {
      "source": "/api/registry",
      "destination": "/api/registry-db"
    },
    {
      "source": "^/(?!api/)(.*)$",
      "destination": "/api/index"
    }
  ]
}